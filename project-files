Make sure port 8080 is allowed in sg

project deployment:

apiVersion: apps/v1
kind: Deployment
metadata:
  name: java-deployment
  labels:
     app: java

spec:
  replicas: 2
  selector:
    matchLabels:
      app: java

  template:
    metadata:
      labels:
        app: java
    spec:
      containers:
      - name: java
        image: public.ecr.aws/m2r5y6g7/tony-repo:latest
        imagePullPolicy: Always
        ports:
        - containerPort: 8080
  strategy:
    type: RollingUpdate
    rollingUpdate:
      maxSurge: 1
      maxUnavailable: 1


service file:

apiVersion: v1
kind: Service
metadata:
  name: java-service
  labels:
    app: java
spec:
  selector:
    app: java

  ports:
    - port: 8080
      targetPort: 8080

  type: LoadBalancer
